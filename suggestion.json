  {
    "matcher": "(?i)^(?:.*)\\bmake\\b",
    "test_ok": ["GNU make", "bla bla MAKE", "make package"],
    "test_fail": ["makers", "amake", "bla bla noMake"],
    "packages": [{ "name": "make", "manager": "apt" }]
  },
  {
    "matcher": "(?i)^(?:.*)\\b(?:lib)?curl\\b",
    "test_ok": ["libcurl", "I might need curl"],
    "test_fail": ["Nanacurl", "somecurl is needed", "thisisnotcurl"],
    "packages": [{ "name": "libcurl-dev", "manager": "apt" }]
  },
  {
    "matcher": "(?i)^(?:.*)\\b(subversion|svn)\\b",
    "test_ok": [
      "Do you really wanna know what I need? subversion.",
      "svn seems a really good idea",
      "I might need svn"
    ],
    "test_fail": ["nosvnpleaseno", "subversionista"],
    "packages": [{ "name": "subversion", "manager": "apt" }]
  },
  {
    "matcher": "(?i)^(?:.*)\\bgit\\b",
    "test_ok": ["git", "We need git"],
    "test_fail": ["github", "Nogit"],
    "packages": [{ "name": "subversion", "manager": "apt" }]
  },
  {
    "matcher": "(?i)^(?:.*)\\bgit\\b",
    "test_ok": ["git", "We need git"],
